{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "UI markup language",
    "patterns": [
        {
            "include": "#comment"
        }
    ],
    "repository": {
        "comment": {
            "patterns": [
                {
                    "match": "(^#)([^>].*)?$\\n?",
                    "name": "comment.line.number-sign.uml"
                }
            ]
        }
    },
    "scopeName": "source.uml"
}
